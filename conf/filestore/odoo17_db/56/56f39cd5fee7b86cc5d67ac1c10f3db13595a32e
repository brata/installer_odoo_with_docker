node=node.parentNode;}
return false;},});function firstChild(node){while(node.firstChild){node=node.firstChild;}
return node;}
function lastChild(node){while(node.lastChild){node=node.lastChild;}
return node;}
function nodeLength(node){if(node.nodeType===Node.TEXT_NODE){return node.nodeValue.length;}else{return node.childNodes.length;}}
$.fn.extend({focusIn:function(){if(this.length){const selection=this[0].ownerDocument.getSelection();selection.removeAllRanges();const range=new Range();const node=firstChild(this[0]);range.setStart(node,0);range.setEnd(node,0);selection.addRange(range);}
return this;},focusInEnd:function(){if(this.length){const selection=this[0].ownerDocument.getSelection();selection.removeAllRanges();const range=new Range();const node=lastChild(this[0]);const length=nodeLength(node);range.setStart(node,length);range.setEnd(node,length);selection.addRange(range);}
