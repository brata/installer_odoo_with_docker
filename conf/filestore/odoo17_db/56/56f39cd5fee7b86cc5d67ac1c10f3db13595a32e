_onChangeCustomBorderWidth(ev){const value=ev.target.value;if(parseInt(value)>=0){this.$link.css('border-width',value+'px');}}
_onKeyPressCustomBorderWidth(ev){if(ev.key==="Enter"){this._onChangeCustomBorderWidth(ev);}}
