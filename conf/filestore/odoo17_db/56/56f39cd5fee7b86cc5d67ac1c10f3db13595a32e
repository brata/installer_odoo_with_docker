return this;},selectElement:function(){if(this.length){const selection=this[0].ownerDocument.getSelection();selection.removeAllRanges();const element=this[0];const parent=element.parentNode;const offsetStart=Array.from(parent.childNodes).indexOf(element);const range=new Range();range.setStart(parent,offsetStart);range.setEnd(parent,offsetStart+1);selection.addRange(range);}
return this;},});return __exports;});;

