async _updateState(){await super._updateState(...arguments);this.customColors={};this.PREFIXES={'color':'text-','background-color':'bg-',};}
async start(){const ret=await super.start(...arguments);this.$el.on('click','we-select we-button',this._onPickSelectOption.bind(this));this.$el.on('click','we-checkbox',this._onClickCheckbox.bind(this));this.$el.on('change','.link-custom-color-border input',this._onChangeCustomBorderWidth.bind(this));this.$el.on('keypress','.link-custom-color-border input',this._onKeyPressCustomBorderWidth.bind(this));this.$el.on('click','we-select [name="link_border_style"] we-button',this._onBorderStyleSelectOption.bind(this));this.$el.on('input','input[name="label"]',this._onLabelInput.bind(this));this._setSelectOptionFromLink();this._updateOptionsUI();if(!this.linkEl.href&&this.state.url){this.__onURLInput();}
