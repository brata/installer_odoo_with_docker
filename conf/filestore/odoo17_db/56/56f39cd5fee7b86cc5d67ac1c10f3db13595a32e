__onURLInput(){super.__onURLInput(...arguments);this.props.wysiwyg.odooEditor.historyPauseSteps('_onURLInput');this._syncContent();this._adaptPreview();this.props.wysiwyg.odooEditor.historyUnpauseSteps('_onURLInput');}
