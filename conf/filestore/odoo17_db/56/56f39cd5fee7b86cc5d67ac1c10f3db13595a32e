this._dropzonesContainer.append(dropzoneBox);this._dropzoneHintContainer.append(dropzoneHintBox);}}
_stopDropzones(movableElement){this._editable.classList.remove('oe-editor-dragging');this._dropzonesContainer.replaceChildren();this._dropzoneHintContainer.replaceChildren();if(this._currentDropHintElementPosition){const[position,focusElelement]=this._currentDropHintElementPosition;this._currentDropHintElementPosition=undefined;const previousParent=movableElement.parentElement;if(position==='top'){focusElelement.before(movableElement);}else if(position==='bottom'){focusElelement.after(movableElement);}
