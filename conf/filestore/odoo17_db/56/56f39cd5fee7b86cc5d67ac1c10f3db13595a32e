/* /web_editor/static/src/js/wysiwyg/wysiwyg_jquery_extention.js */
odoo.define('@web_editor/js/wysiwyg/wysiwyg_jquery_extention',[],function(require){'use strict';let __exports={};$.extend($.expr[':'],{o_editable:function(node,i,m){while(node){if(node.className&&typeof node.className==="string"){if(node.className.indexOf('o_not_editable')!==-1){return false;}
