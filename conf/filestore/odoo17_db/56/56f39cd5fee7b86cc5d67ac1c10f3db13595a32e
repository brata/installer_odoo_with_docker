_renderElement(){this.$editable=this.options.editable||$('<div class="note-editable">');if(typeof this.options.fieldId!=='undefined'&&!this.options.inIframe){this.$editable.attr('id',this.options.fieldId);this.$editable.attr('contenteditable',true);}
this.$root=this.$editable;if(this.options.height){this.$editable.height(this.options.height);}
