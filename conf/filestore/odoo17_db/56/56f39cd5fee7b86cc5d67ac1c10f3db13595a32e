_getLinkCustomBorderStyle(){return this.$el.find('.link-custom-color-border we-button.active').data('value')||'';}
_getLinkCustomFill(){return this.customColors['background-color'];}
_getLinkCustomClasses(){let textClass=this.customColors['color'];const colorPickerFg=this.colorpickers['color'].colorNames;if(!textClass||!colorPickerFg||!weUtils.computeColorClasses(colorPickerFg,'text-').includes(textClass)){textClass='';}
